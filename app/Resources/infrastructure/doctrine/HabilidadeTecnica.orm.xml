<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HabilidadeTecnica" table="habilidade_tecnica">
    <id name="idHabilidadeTecnica" type="integer" column="id_habilidade_tecnica">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descricao" type="string" column="descricao" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="idCandidato" target-entity="Candidato" mapped-by="idHabilidadeTecnica" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
